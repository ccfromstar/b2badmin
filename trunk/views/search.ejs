<table id=_searchDIV style="display:none">
    <tr>
        <td><select onchange='_setSColumn(this.value)' id='s_column' class='input-sm selectpicker'><option value='-'><span class='glyphicon glyphicon-refresh' style='margin-right:5px'></span>数据加载中</option></select></td>
        <td style='padding-left:0px'><input type='hidden' class='input-sm form-control' id='colname' disabled ></input></td>
        <td style='padding-left:5px'>
            <select id='s_symbol' class='input-sm selectpicker'>
                <option value='like'>包含</option>
                <option value='='>等于</option>
                <option value='>'>大于</option>
                <option value='<'>小于</option>
                <option value='!='>不等于</option>
            </select>
        </td>
        <td style='padding-left:5px'><input type='text' class='input-sm form-control' id='s_value'></input></td>
    </tr>
</table>