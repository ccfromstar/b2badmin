<%
var l = log;
var tmp = l.split("|");
var a1 = tmp[0].split("@");
var a2 = tmp[1].split("@");
%>
<table style='margin-top:5px;width:250px;' class='table table-striped table-bordered'>
<tr><th>排名</th><th>IP地址</th><th>访问量(次)</th></tr>
<%for(var i=0;i<a1.length;i++){%>
<tr>
	<td><%=(i+1)%></td>
	<td><%=a1[i]%></td>
	<td><%=a2[i]%></td>
</tr>
<%}%>
</table>